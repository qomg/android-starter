{
  "extends": [
    "config:base"
  ],
  "commitBodyTable": true,
  "semanticCommits": "enabled",
  "labels": ["dependencies"],
  "assignees": ["qomg"],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch"
      ],
      "groupName": "all dependencies",
      "groupSlug": "all-deps"
    }
  ]
}
